    {
      "name": "cacheable-queries",
      "description": "Indexes the whole document corpus using a setup that will lead to a larger indexing throughput than the default settings and produce a smaller index (higher compression rate). Document ids are unique so all index operations are append only. After that a couple of queries are run.",
      "default": true,
      "schedule": [
        {
          "operation": "delete-index"
        },
        {
          "operation": {
            "operation-type": "create-index",
            "settings": {%- if index_settings is defined %} {{index_settings | tojson}} {%- else %} {
              "index.codec": "best_compression",
              "index.refresh_interval": "30s",
              "index.translog.flush_threshold_size": "4g",
              "index.queries.cache.enabled": "true"
            }{%- endif %}
          }
        },
        {
          "operation": "index",
          "warmup-time-period": 240,
          "clients": {{bulk_indexing_clients | default(8)}},
          "ignore-response-error-level": "{{error_level | default('non-fatal')}}"
        },
        {
          "name": "refresh-after-index",
          "operation": "refresh"
        },
        {
          "operation": {
            "operation-type": "force-merge",
            "request-timeout": 7200{%- if force_merge_max_num_segments is defined %},
            "max-num-segments": {{ force_merge_max_num_segments | tojson }}
            {%- endif %}
          }
        },
        {
          "name": "refresh-after-force-merge",
          "operation": "refresh"
        },
        {
          "operation": "wait-until-merges-finish"
        },

        {
          "name":"1d", 
          "operation":"1d",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 10
        },
        {
          "name":"2d", 
          "operation":"2d",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 10
        },
        {
          "name":"4d", 
          "operation":"4d",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 10
        },

        {
          "name":"1w", 
          "operation":"1w",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 10
        },
        {
          "name":"2w", 
          "operation":"2w",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 5
        },
        {
          "name":"3w", 
          "operation":"3w",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 3
        },
        {
          "name":"1m", 
          "operation":"1m",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 1000,
          "target-throughput": 2.5
        },
        {
          "name":"6w", 
          "operation":"6w",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 500,
          "target-throughput": 1.7
        },
        {
          "name":"2m", 
          "operation":"2m",
          "settings": {
            "index.requests.cache.enable":true
          }, 
          "warmup-iterations": 50, 
          "iterations": 500,
          "target-throughput": 1.2
        }
      ]
    }