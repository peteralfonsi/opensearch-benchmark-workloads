    {
      "name": "cacheable-queries",
      "description": "Indexes the whole document corpus using a setup that will lead to a larger indexing throughput than the default settings and produce a smaller index (higher compression rate). Document ids are unique so all index operations are append only. After that a couple of queries are run.",
      "default": true,
      "schedule": [
        {
          "operation": "delete-index"
        },
        {
          "operation": {
            "operation-type": "create-index",
            "settings": {%- if index_settings is defined %} {{index_settings | tojson}} {%- else %} {
              "index.codec": "best_compression",
              "index.refresh_interval": "30s",
              "index.translog.flush_threshold_size": "4g"
            }{%- endif %}
          }
        },
        {
          "name": "check-cluster-health",
          "operation": {
            "operation-type": "cluster-health",
            "index": "nyc_taxis",
            "request-params": {
              "wait_for_status": "{{cluster_health | default('green')}}",
              "wait_for_no_relocating_shards": "true"
            },
            "retry-until-success": true
          }
        },
        {
          "operation": "index",
          "warmup-time-period": 240,
          "clients": {{bulk_indexing_clients | default(8)}},
          "ignore-response-error-level": "{{error_level | default('non-fatal')}}"
        },
        {
          "name": "refresh-after-index",
          "operation": "refresh"
        },
        {
          "operation": {
            "operation-type": "force-merge",
            "request-timeout": 7200{%- if force_merge_max_num_segments is defined %},
            "max-num-segments": {{ force_merge_max_num_segments | tojson }}
            {%- endif %}
          }
        },
        {
          "name": "refresh-after-force-merge",
          "operation": "refresh"
        },
        {
          "operation": "wait-until-merges-finish"
        },
        {
          "name":"cheap-passenger-count",
          "operation":"cheap-passenger-count", 
          "settings": {
            "index.requests.cache.enable":true
          },
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "name":"cheap-passenger-count-no-cache",
          "operation":"cheap-passenger-count", 
          "settings": {
            "index.requests.cache.enable":false
          },
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-tip-amount", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-tip-amount-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-fare-amount", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-fare-amount-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-total-amount", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-total-amount-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-pickup", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-pickup-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-dropoff", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"cheap-dropoff-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "name":"expensive-1",
          "operation":"expensive-1", 
          "settings":{
            "index.requests.cache.enable":true
          },
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "name":"expensive-1-no-cache",
          "operation":"expensive-1", 
          "settings":{
            "index.requests.cache.enable":false
          },
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"expensive-2", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"expensive-2-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"expensive-3", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"expensive-3-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"expensive-4-no-cache", 
          "warmup-iterations": 0, 
          "iterations": 10
        },
        {
          "operation":"expensive-4", 
          "warmup-iterations": 0, 
          "iterations": 10
        }
      ]
    }
